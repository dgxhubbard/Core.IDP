
@page "/logout"

@model LogoutModel

@namespace Core.IDP.Pages


<div>
    <div class="lead">
        <h1>Logout</h1>
        <p>Would you like to logout?</p>
    </div>

    <form method="post" >
        <input type="hidden" name="LogoutId"  />
        
        <div class="form-group">
            <button class="btn btn-primary" type="submit" >Yes</button>
        </div>
    </form>
</div>